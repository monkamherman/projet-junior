{
  "license": "MIT",
  "main": "index.html",
  "keywords": [
    "list",
    "of",
    "keywords"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist && vite build",
    "build:test": "NODE_ENV=production rimraf /tmp/dist && vite build --outDir /tmp/dist",
    "docker:build": "docker build -t react-started .",
    "docker:start": "docker run -p 4001:3000 react-started",
    "clean": "rimraf dist",
    "start": "vite preview",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "test": "vitest --run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:preview": "serve coverage -l 4003",
    "test:clean": "rimraf coverage",
    "ci": "bun run lint && bun run test && bun run build",
    "docs": "typedoc",
    "docs:preview": "serve docs -l 4002",
    "docs:clean": "rimraf docs",
    "reinstall": "bun run clean && rimraf node_modules && bun install",
    "commitlint": "commitlint",
    "release": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "upgrade": "yarn upgrade-interactive --latest"
  },
  "lint-staged": {
    "*.{ts, js, tsx, jsx}": [
      "eslint --fix --max-warnings=10 --cache --config eslint.config.js",
      "prettier --write --config .prettierrc"
    ],
    "*.{json,css}": [
      "prettier --write"
    ],
    "*.md": [
      "markdownlint --fix",
      "prettier --write"
    ]
  },
